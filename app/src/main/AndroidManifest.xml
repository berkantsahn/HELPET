<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.helpetapplicationgmail.helpet">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Likes.LikesActivity" />
        <activity android:name=".Profile.ProfileActivity" />
        <activity android:name=".Profile.PrivacyPolicyFragment"/>
        <activity android:name=".Search.SearchActivity" />
        <activity android:name=".Share.ShareActivity" />
        <activity android:name=".Profile.AccountSettingsActivity" />
        <activity android:name=".Others.LoginActivity" />
        <activity android:name=".Others.RegisterActivity" />
        <activity android:name=".Others.ForgotPassword"/>
        <activity android:name=".Share.NextActivity"/>
        <activity android:name=".Share.SelectionFragment"/>
        <activity android:name=".Share.SharePostActivity"/>
        <activity android:name=".Utils.ViewProfileActivity"/>
        <activity android:name=".Share.NextHelpActivity"/>
        <activity android:name=".Share.ShareHelpActivity"/>

    </application>
</manifest>